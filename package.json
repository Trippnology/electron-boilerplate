{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "description": "Starter for your Electron application",
  "version": "0.0.0",
  "private": true,
  "author": "Mr. Gumby <mr.gumby@example.com>",
  "copyright": "Â© 2017, Gumby inc.",
  "homepage": "http://example.com",
  "main": "app/main.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test",
    "unit": "electron-mocha temp/specs.js --renderer --color --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test && webpack --config=build/webpack.e2e.config.js --env=test",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder",
    "build": "webpack --config=build/webpack.app.config.js --env=production && electron-builder",
    "wdio": "wdio run ./wdio.conf.js"
  },
  "dependencies": {
    "fs-jetpack": "5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.4",
    "@electron/remote": "2.1.2",
    "@testing-library/webdriverio": "3.2.1",
    "@wdio/cli": "9.0.7",
    "@wdio/local-runner": "9.0.7",
    "@wdio/mocha-framework": "9.0.6",
    "@wdio/spec-reporter": "9.0.7",
    "babel-loader": "9.1.3",
    "chai": "4.5.0",
    "css-loader": "7.1.2",
    "electron": "26.6.10",
    "electron-builder": "24.13.3",
    "electron-mocha": "11.0.2",
    "mocha": "9.2.2",
    "source-map-support": "0.5.21",
    "spectron": "16.0.0",
    "style-loader": "4.0.0",
    "wdio-electron-service": "7.0.1",
    "wdio-wait-for": "3.0.11",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "6.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
